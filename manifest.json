{

	"manifest_version": 2,

	"name": "eego extension",
	"version": "0.0.1",
	"description": "Create visual collections of multimedia and resources",
	"browser_action": {
		"default_title": "Collect this",
		"default_popup": "popup.html",
		"default_icon": {
			"19": "images/icon19.png",
			"38": "images/icon38.png"
		}
	},
	"icons": {
		"16": "images/icon16.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	},
	"permissions": [
		"activeTab",
		"storage",
		"tabs",
		"https://*.eego.co"
  ]

}
